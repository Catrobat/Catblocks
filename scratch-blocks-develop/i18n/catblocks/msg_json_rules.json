{
  "@metadata": {
    "author": "Andreas.Karner@student.tugraz.at",
    "description": "Generator rules for msg JSON files, source are catroid string xmls"
  },
  "@motion" : "motion blocks",
  "MOTION_PLACEATXY": "${brick_place_at} ${x_label} %1 ${y_label} %2",
  "MOTION_SETXTO": "${brick_set_x} %1",
  "MOTION_SETYTO": "${brick_set_y} %1",
  "MOTION_CHANGEXBY": "${brick_change_x_by} %1",
  "MOTION_CHANGEYBY": "${brick_change_y_by} %1",
  "MOTION_GOTO" : "${brick_go_to} %1",
  "MOTION_IFONEDGEBOUNCE": "${brick_if_on_edge_bounce}",
  "MOTION_MOVESTEPS": "${brick_move} %1 ${brick_move_n_step_plural.other}",
  "MOTION_TURNRIGHTDEGREES": "${brick_turn_right} %1 ${degrees}",
  "MOTION_TURNLEFTDEGREES": "${brick_turn_left} %1 ${degrees}",
  "MOTION_POINTINDIRECTIONDEGREES": "${brick_point_in_direction} %1 ${degrees}",
  "MOTION_POINTTOWARDS": "${brick_point_to} %1",
  "MOTION_SETROTATIONSTYLE": "${brick_set_physics_object_type} %1",
  "MOTION_GLIDESECONDTOXY": "${brick_glide} %1 ${phiro_seconds} ${brick_glide_to_x} %2 ${y_label} %3",
  "MOTION_GOBACKLAYER": "${brick_go_back} %1 ${brick_go_back_layer_plural.one}",
  "MOTION_GOTOFRONT": "${brick_come_to_front}",
  "MOTION_VIBRATEFORSECOND": "${brick_vibration} %1 ${phiro_seconds}",
  "MOTION_SETYOURMOTIONTYPETO": "${brick_set_physics_object_type} %1",
  "MOTION_SETVELOCITYTO": "${brick_set_velocity_to} ${x_label} %1 ${y_label} %2 ${brick_set_velocity_unit}",
  "MOTION_SPINLEFTDEGREESSECOND": "${brick_rotate_left} %1 ${brick_turn_speed_unit}",
  "MOTION_SPINRIGHTDEGREESSECOND": "${brick_rotate_right} %1 ${brick_turn_speed_unit}",
  "MOTION_SETGRAVITYFORALLACTORSANDOBJECTSTO": "${brick_set_gravity_to} ${x_label} %1 and ${y_label} %2 ${brick_set_gravity_unit}",
  "MOTION_SETMASSTOKILOGRAM": "${brick_set_mass} %1 ${brick_set_mass_unit}",
  "MOTION_SETBOUNCEFACTORTO": "${brick_set_bounce_factor} %1 %",
  "MOTION_SETFRICTIONTO": "${brick_set_friction} %1 %",

  "@control": "control blocks",
  "CONTROL_FOREVER": "${brick_forever}",
  "CONTROL_REPEAT": "${brick_repeat} %1",
  "CONTROL_IF": "${brick_if_begin} %1",
  "CONTROL_ELSE": "${brick_if_else}",
  "CONTROL_STOP": "${brick_if_end}",
  "CONTROL_STOP_ALL": "${brick_stop_all_scripts}",
  "CONTROL_STOP_THIS": "${brick_stop_this_script}",
  "CONTROL_STOP_OTHER": "${brick_stop_other_scripts}",
  "CONTROL_WAIT": "${brick_wait} %1 ${phiro_seconds}"
}