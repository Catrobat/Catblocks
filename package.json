  "scripts": {
    "start": "yarn playground:server --print-label",
    "playground:server": "export NODE_ENV=development TYPE=playground && webpack-dev-server",
    "playground:prod": "export NODE_ENV=production TYPE=playground && webpack-dev-server",
    "playground:build": "export NODE_ENV=production TYPE=playground && webpack",
    "playground:dev": "export NODE_ENV=development TYPE=playground && webpack",
    "playground:test": "yarn playground:build && jest -c jest_config.js",
    "share:server": "export NODE_ENV=development TYPE=share && webpack-dev-server",
    "share:prod": "export NODE_ENV=production TYPE=share && webpack-dev-server",
    "share:build": "export NODE_ENV=production TYPE=share && webpack",
    "share:dev": "export NODE_ENV=development TYPE=share && webpack",
    "@useless...watch:build": "NODE_ENV=development && webpack --watch",
    "translate": "node i18n/create_json.js && node i18n/create_msg.js",
    "toolbox": "node create_toolbox.js",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.3.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest-html-reporters": "^1.2.1",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.8.0",
    "scratch-blocks": "latest",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "jest-environment-node": "^24.9.0",
    "jest-puppeteer": "^4.3.0",
    "puppeteer": "^2.0.0",
    "rimraf": "^3.0.0",
    "xml2json": "^0.11.2"
  }
}
